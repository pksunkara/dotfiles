{
  "paths": [
    {
      "source": "vim",
      "destination": "$HOME/.vim"
    },
    {
      "source": "vim/vimrc",
      "destination": "$HOME/.vimrc"
    },
    {
      "source": "fish",
      "destination": "$HOME/.config/fish"
    },
    {
      "source": "git/.gitconfig",
      "destination": "$HOME/.gitconfig"
    },
    {
      "source": "git/.gitignore",
      "destination": "$HOME/.gitignore"
    }
  ]
}
