[
  // cmd+m, cmd+g, cmd+e, cmd+r
  //#region Editor -> Redo
  {
    "key": "shift+cmd+z",
    "command": "-redo",
    "when": "isMac"
  },
  {
    "key": "cmd+y",
    "command": "redo",
    "when": "isMac"
  },
  {
    "key": "shift+ctrl+z",
    "command": "-redo",
    "when": "!isMac"
  },
  //#endregion
  //#region Files Explorer
  // New file
  {
    "key": "cmd+a",
    "command": "explorer.newFile",
    "when": "isMac && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "ctrl+a",
    "command": "explorer.newFile",
    "when": "!isMac && !explorerResourceReadonly && !inputFocus"
  },
  // New folder
  {
    "key": "ctrl+a",
    "command": "explorer.newFolder",
    "when": "isMac && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "cmd+a",
    "command": "explorer.newFolder",
    "when": "!isMac && !explorerResourceReadonly && !inputFocus"
  },
  // New from template
  {
    "key": "alt+a",
    "command": "extension.newModuleFromTemplate",
    "when": "!explorerResourceReadonly && !inputFocus"
  },
  // Rename
  {
    "key": "enter",
    "command": "-renameFile",
    "when": "isMac && filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "cmd+r",
    "command": "renameFile",
    "when": "isMac && filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "f2",
    "command": "-renameFile",
    "when": "!isMac && filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "ctrl+r",
    "command": "renameFile",
    "when": "!isMac && filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  // Open
  {
    "key": "cmd+down",
    "command": "-explorer.openAndPassFocus",
    "when": "isMac && filesExplorerFocus && foldersViewVisible && !explorerResourceIsFolder && !inputFocus"
  },
  {
    "key": "enter",
    "command": "explorer.openAndPassFocus",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsFolder && !inputFocus"
  },
  // Move file to trash
  {
    "key": "ctrl+backspace",
    "command": "moveFileToTrash",
    "when": "!isMac && explorerResourceMoveableToTrash && filesExplorerFocus && foldersViewVisible && !explorerResourceReadonly && !inputFocus"
  },
  // Delete file when trash exists
  {
    "key": "alt+cmd+backspace",
    "command": "-deleteFile",
    "when": "isMac && filesExplorerFocus && foldersViewVisible && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "shift+cmd+backspace",
    "command": "deleteFile",
    "when": "isMac && filesExplorerFocus && foldersViewVisible && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "shift+delete",
    "command": "deleteFile",
    "when": "isMac && filesExplorerFocus && foldersViewVisible && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "shift+ctrl+backspace",
    "command": "deleteFile",
    "when": "!isMac && filesExplorerFocus && foldersViewVisible && !explorerResourceReadonly && !inputFocus"
  },
  // Delete file when no trash
  {
    "key": "delete",
    "command": "deleteFile",
    "when": "isMac && !explorerResourceMoveableToTrash && filesExplorerFocus && foldersViewVisible && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "ctrl+backspace",
    "command": "deleteFile",
    "when": "!isMac && !explorerResourceMoveableToTrash && filesExplorerFocus && foldersViewVisible && !explorerResourceReadonly && !inputFocus"
  },
  //#endregion
  //#region Editor Groups
  {
    "key": "cmd+k cmd+m",
    "command": "-workbench.action.toggleMaximizeEditorGroup",
    "when": "isMac"
  },
  {
    "key": "cmd+k cmd+m",
    "command": "workbench.action.toggleEditorWidths",
    "when": "isMac"
  },
  {
    "key": "ctrl+k ctrl+m",
    "command": "-workbench.action.toggleMaximizeEditorGroup",
    "when": "!isMac"
  },
  {
    "key": "ctrl+k ctrl+m",
    "command": "workbench.action.toggleEditorWidths",
    "when": "!isMac"
  },
  //#endregion
  //#region Panel
  {
    "key": "cmd+k cmd+p",
    "command": "workbench.action.toggleMaximizedPanel",
    "when": "isMac"
  },
  {
    "key": "ctrl+k ctrl+p",
    "command": "workbench.action.toggleMaximizedPanel",
    "when": "!isMac"
  },
  //#endregion
  //#region Keybindings
  {
    "key": "cmd+k cmd+s",
    "command": "-workbench.action.openGlobalKeybindings",
    "when": "isMac"
  },
  {
    "key": "cmd+k cmd+k",
    "command": "workbench.action.openGlobalKeybindings",
    "when": "isMac"
  },
  {
    "key": "ctrl+k ctrl+s",
    "command": "-workbench.action.openGlobalKeybindings",
    "when": "!isMac"
  },
  {
    "key": "ctrl+k ctrl+k",
    "command": "workbench.action.openGlobalKeybindings",
    "when": "!isMac"
  },
  //#endregion
  // Make keybindings similar
  {
    "key": "cmd+g",
    "command": "-editor.action.nextMatchFindAction",
    "when": "isMac"
  },
  {
    "key": "cmd+shift+g",
    "command": "-editor.action.previousMatchFindAction",
    "when": "isMac"
  },
  {
    "key": "cmd+alt+f",
    "command": "-editor.action.startFindReplaceAction",
    "when": "isMac"
  },
  {
    "key": "cmd+h",
    "command": "editor.action.startFindReplaceAction",
    "when": "isMac && (editorFocus || editorIsOpen)"
  },
  {
    "key": "cmd+e",
    "command": "-actions.findWithSelection",
    "when": "isMac"
  },
  {
    "key": "ctrl+m",
    "command": "-editor.action.toggleTabFocusMode",
    "when": "!isMac"
  },
  {
    "key": "cmd+shift+m",
    "command": "editor.action.toggleTabFocusMode",
    "when": "!isMac"
  },
  {
    "key": "ctrl+g",
    "command": "-workbench.action.gotoLine",
    "when": "!isMac"
  },
  {
    "key": "cmd+g",
    "command": "workbench.action.gotoLine",
    "when": "!isMac"
  },
  {
    "key": "cmd+e",
    "command": "-workbench.action.quickOpen",
    "when": "!isMac"
  },
  {
    "key": "ctrl+r",
    "command": "-workbench.action.openRecent",
    "when": "!isMac"
  },
  {
    "key": "cmd+r",
    "command": "workbench.action.openRecent",
    "when": "!isMac"
  },
  {
    "key": "ctrl+k ctrl+p",
    "command": "-workbench.action.showAllEditors",
    "when": "!isMac"
  },
  {
    "key": "ctrl+shift+tab",
    "command": "workbench.action.showAllEditors",
    "when": "!isMac"
  },
  // Primary sidebar
  {
    "key": "shift+cmd+f",
    "command": "-workbench.view.search"
  },
  {
    "key": "alt+f",
    "command": "workbench.view.search"
  },
  // Projects
  {
    "key": "ctrl+cmd+p",
    "command": "projects.open"
  },
  {
    "key": "alt+cmd+p",
    "command": "-projects.open"
  },
  {
    "key": "ctrl+cmd+g",
    "command": "projects.openGroup"
  },
  {
    "key": "shift+alt+cmd+x",
    "command": "-projects.openGroup"
  },
  {
    "key": "ctrl+cmd+x",
    "command": "projects.switchGroup"
  },
  {
    "key": "alt+cmd+x",
    "command": "-projects.switchGroup"
  }
]
